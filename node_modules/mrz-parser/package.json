{
  "name": "mrz-parser",
  "version": "0.0.1",
  "description": "Parse MRZ (Machine Readable Zone) from identity documents",
  "main": "./src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "lint": "npx eslint .",
    "eslint-fix": "npm run lint -- --fix",
    "test": "jest src",
    "test-coverage": "jest src --coverage",
    "build": "tsc",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && npx jest-coverage-badges --input ./test-coverage/coverage-summary.json --output badges"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vvorld/mrz-parser"
  },
  "keywords": [
    "mrz"
  ],
  "author": "getid",
  "bugs": {
    "url": "https://github.com/vvorld/mrz-parser/issues"
  },
  "homepage": "https://github.com/vvorld/mrz-parser#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^25.1.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^22.6.0",
    "jest-cli": "^25.1.0",
    "jest": "^25.1.0"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@vorld/base-service": "^1.5.9",
    "cors": "^2.8.5",
    "i18n-iso-countries": "^6.2.2",
    "jest-coverage-badges": "^1.1.2",
    "moment": "^2.29.1",
    "nodemon": "^2.0.6"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "node_modules",
      "__fixtures__"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "json-summary",
      "text"
    ],
    "coverageDirectory": "test-coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
